{
    "name": "Bookmark Sort",
    "description": "This extension is made for demonstration purposes",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "bookmarks"
    ],
    "action": {
        "default_icon": {
            "16": "bs16.png",
            "48": "bs48.png",
            "128": "bs128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}