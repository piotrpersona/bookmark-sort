{
    "name": "Bookmark Sort",
    "description": "This extension is made for demonstration purposes",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "bookmarks"
    ],
    "action": {
        "default_icon": "bs128.png",
        "default_popup": "index.html",
        "default_tile": "Bookmark Sort"
    },
    "icons": {
        "16": "bs128.png",
        "48": "bs128.png",
        "128": "bs128.png"
    },
    "background": {
        "service_worker": "static/js/background.js"
    }
}